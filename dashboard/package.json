{
    "name": "wakedock-dashboard",
    "version": "1.0.0",
    "description": "WakeDock Web Dashboard",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "build:debug": "vite build --mode development --debug",
        "preview": "vite preview",
        "start": "node build/index.js",
        "start:debug": "node --inspect=0.0.0.0:9229 build/index.js",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test:build": "npm run build && test -f build/index.js && echo 'Build test passed' || echo 'Build test failed'"
    },
    "devDependencies": {
        "@sveltejs/adapter-node": "^1.3.1",
        "@sveltejs/kit": "^1.20.4",
        "@tsconfig/svelte": "^5.0.0",
        "svelte": "^4.0.5",
        "svelte-check": "^3.4.3",
        "typescript": "^5.0.0",
        "vite": "^4.4.2"
    },
    "dependencies": {
        "@fontsource/inter": "^5.0.8",
        "chart.js": "^4.3.0",
        "lucide-svelte": "^0.262.0",
        "polka": "^0.5.2"
    }
}
